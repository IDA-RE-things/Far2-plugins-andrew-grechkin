#include <winver.h>
#include "../include/version.h"

//
// Version Information resources
//
//LANGUAGE LANG_USER_DEFAULT, SUBLANG_NEUTRAL
1 VERSIONINFO
	FILEVERSION RC_FILEVERSION
	PRODUCTVERSION RC_FILEVERSION
	FILEOS VOS_NT_WINDOWS32
	FILETYPE VFT_APP
BEGIN
	BLOCK "StringFileInfo"
	BEGIN
		BLOCK "000004E4"
		BEGIN
			VALUE "CompanyName", "Andrew Grechkin"
			VALUE "FileVersion", RC_FILEVERSION_STRING
			VALUE "FileDescription", "Sort strings FAR3 plugin"
			VALUE "InternalName", "sortstr.dll"
			VALUE "LegalCopyright", "GNU GPL (2013) Andrew Grechkin"
			VALUE "ProductName", "Sort strings in editor"
		END
	END
	BLOCK "VarFileInfo"
	BEGIN
		VALUE "Translation", 0, 0x4e4
	END
END
