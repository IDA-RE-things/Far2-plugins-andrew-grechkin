<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="hdlink-x64" />
		<Option pch_mode="2" />
		<Option compiler="gnu_gcc_compiler_64" />
		<Build>
			<Target title="Debug">
				<Option output="bin\Debug\hdlink-x64" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Debug-x64\" />
				<Option type="1" />
				<Option compiler="gnu_gcc_compiler_64" />
				<Option parameters='/r /d /m &quot;T:\#drv&quot;' />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-g" />
					<Add option="-DDEBUG" />
					<Add option="-D_DEBUG" />
					<Add option="-DUNICODE" />
					<Add option="-D_UNICODE" />
					<Add option="-DWIDE=1" />
					<Add option="-DNoStdNew" />
					<Add option="-DNoStlString" />
				</Compiler>
				<Linker>
					<Add library="advapi32" />
					<Add library="win_def_ds" />
					<Add library="ntdll" />
					<Add library="kernel32" />
					<Add library="gdi32" />
					<Add library="mpr" />
					<Add library="shell32" />
					<Add library="shlwapi" />
					<Add library="user32" />
					<Add library="winmm" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="..\hdlink-x64" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Release-x64\" />
				<Option type="1" />
				<Option compiler="gnu_gcc_compiler_64" />
				<Option parameters='/r /d /m &quot;T:\#drv&quot;' />
				<Compiler>
					<Add option="-Os" />
					<Add option="-Wall" />
					<Add option="-DUNICODE" />
					<Add option="-D_UNICODE" />
					<Add option="-DWIDE=1" />
					<Add option="-DNoStdNew" />
					<Add option="-DNoStlString" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="win_def_s" />
					<Add library="ntdll" />
					<Add library="advapi32" />
					<Add library="kernel32" />
					<Add library="gdi32" />
					<Add library="mpr" />
					<Add library="shell32" />
					<Add library="shlwapi" />
					<Add library="user32" />
					<Add library="winmm" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-fomit-frame-pointer" />
			<Add option="-Wall" />
			<Add option="-mno-cygwin" />
			<Add option="-fstrict-aliasing" />
			<Add option="-fno-rtti" />
			<Add option="-fno-exceptions" />
			<Add option="-fno-threadsafe-statics" />
		</Compiler>
		<Linker>
			<Add option="-mno-cygwin" />
			<Add option="-Wl,-Map=../hdlink-x64.map" />
		</Linker>
		<Unit filename="main.cpp" />
		<Unit filename="main.h" />
		<Unit filename="main.rc">
			<Option compilerVar="WINDRES" />
		</Unit>
		<Unit filename="version.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
